<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation0"
    app:startDestination="@id/logFragment">
    <fragment
        android:id="@+id/logFragment"
        android:name="com.example.bottomnavigationdemo.LogFragment"
        android:label="log_fragment"
        tools:layout="@layout/log_fragment" >
        <action
            android:id="@+id/action_logFragment_to_register_Fragment"
            app:destination="@id/register_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/register_Fragment"
        android:name="com.example.bottomnavigationdemo.RegisterFragment"
        android:label="register__fragment"
        tools:layout="@layout/register__fragment" >
        <action
            android:id="@+id/action_register_Fragment_to_logFragment"
            app:destination="@id/logFragment" />
    </fragment>
</navigation>